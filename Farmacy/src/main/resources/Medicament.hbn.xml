<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
    <class name="domain.Medicament" table="MEDICAMENTE">
        <id name="id" column="id"> <generator class="increment"/> </id>
        <property name="nume" column="nume"/>
        <property name="categorie" column="categorie"/>
        <property name="prospect" column="prospect"/>
        <property name="pret" column="pret"/>
    </class>
</hibernate-mapping>